<?xml version="1.0" encoding="UTF-8"?>
<persistence xmlns="http://java.sun.com/xml/ns/persistence"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_1_0.xsd"
	version="1.0">

	<persistence-unit name="persistenceUnit"
		transaction-type="RESOURCE_LOCAL">

		<class>org.skillbill.common.Skill</class>
		<class>org.skillbill.common.Mitarbeiter</class>
		<class>org.skillbill.common.Ausschreibung</class>
		<class>org.skillbill.common.MitarbeiterSkill</class>
		<class>org.skillbill.common.AusschreibungSkill</class>




		<exclude-unlisted-classes>true</exclude-unlisted-classes>

	</persistence-unit>

</persistence>